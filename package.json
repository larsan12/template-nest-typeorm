  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "build": "npm run build:ts",
    "build:ts": "tsc",
    "build:check": "tsc --noEmit",
    "tslint": "tslint 'src/**/*.ts' -c tslint.json -p tsconfig.json --fix",
    "start": "node dist/src/main.js",
    "dev": "ts-node --files src/main.ts",
    "migrate:gen": "npx typeorm migration:generate --name",
    "migrate:add": "npx typeorm migration:create --name",
    "migrate:up": "npx typeorm migration:run",
    "migrate:down": "npx typeorm migration:revert",
    "test": "mocha -timeout 10000 -r ts-node/register test/*.test.ts"
  },

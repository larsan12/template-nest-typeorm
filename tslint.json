{
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts"]
  },
  "rules": {
    "interface-name": false,
    "object-literal-sort-keys": false,
    "member-access": [true, "no-public"],
    "no-implicit-dependencies": false,
    "ordered-imports": false,
    "no-console": [true, "warning"],
    "no-debugger": false,
    "semicolon": [true, "never"],
    "radix": false
  }
}
